{
  "rootDir": ".",
    "cache": false,
    "verbose": true,
    "coverageThreshold": {
      "global": {
        "branches": 95,
        "functions": 95,
        "lines": 95,
        "statements": 95
      }
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**"
    ],
    "coverageDirectory": "coverage/",
    "transformIgnorePatterns": [
      ".*/node_modules/(?!(zazen)/).*/"
    ]
}
